{
  "name": "simplcms",
  "version": "0.1.35-canary.11",
  "private": false,
  "sideEffects": [
    "./src/app/simplcms.css"
  ],
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/types.ts",
  "files": [
    "scripts/",
    "src/",
    "types/",
    "README.md",
    "tsconfig.json"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./types": "./src/types/types.ts"
  },
  "bin": {
    "simplcms": "./scripts/cli.js"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build:css": "npx tailwindcss -i ./src/app/index.css -o ./src/app/simplcms.css",
    "build:ts": "tsup src/index.ts --format esm,cjs --dts",
    "build": "tsup && npm run build:css",
    "publish:patch": "npm version patch && npm publish && git push --follow-tags",
    "publish:minor": "npm version minor && npm publish && git push --follow-tags",
    "publish:major": "npm version major && npm publish && git push --follow-tags",
    "publish:canary": "npm version prerelease --preid=canary && npm publish --tag canary && git push --follow-tags",
    "start": "next start",
    "lint": "next lint",
    "document": "npx typedoc --plugin typedoc-plugin-markdown --out ./docs/docs/api --cleanOutputDir false"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.758.0",
    "@radix-ui/react-alert-dialog": "^1.1.4",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.5",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-popover": "^1.1.4",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.1.7",
    "@vercel/sdk": "^1.2.2",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "cloudinary": "^2.5.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.471.2",
    "next-auth": "^4.24.11",
    "react-icons": "^5.4.0",
    "react-markdown": "^9.0.3",
    "react-syntax-highlighter": "^15.6.1",
    "remark-gfm": "^4.0.1",
    "sonner": "^1.7.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.1",
    "@types/react-syntax-highlighter": "^15.5.13",
    "uuid": "^9.0.0",
    "@types/uuid": "^10.0.0"
  },
  "peerDependencies": {
    "mongodb": "^6.12.0",
    "mongoose": "^8.9.5",
    "next": "^13.0.0 || ^14.0.0 || ^15.0.0",
    "react": ">=17.0.0 <19.0.0",
    "react-dom": ">=17.0.0 <19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.1.4",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "tsup": "^8.4.0",
    "typedoc": "^0.28.0",
    "typedoc-plugin-markdown": "^4.5.0",
    "typescript": "^5"
  }
}
